{
  "name": "yyl-ssr-server-webpack-config",
  "version": "0.0.2",
  "description": "webpack config for yyl-ssr server side",
  "main": "webpack.config.js",
  "repository": "https://github.com/jackness1208/yyl-ssr-server-webpack-config.git",
  "author": "jackness1208",
  "license": "MIT",
  "eslintConfig": {
    "globals": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "/dist",
    "/test",
    "**/js/lib"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-yyl-react": "^0.2.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "webpack": "4.44.1"
  },
  "dependencies": {
    "babel-loader": "^8.2.2",
    "ignore-loader": "^0.1.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack-node-externals": "^2.5.2"
  }
}
